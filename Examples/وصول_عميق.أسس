اشمل "مـتم/طـرفية.أسس"؛
اشمل "مـتم/طـرفية.أسس"؛
اشمل "مـحا"؛
مـحا.اشمل_ملف("Alusus/Json"، "جـيسون.أسس")؛

عرف كائن_جيسون: جـيسون = "{\"glossary\": {\"title\": \"example glossary\",\"GlossDiv\": {\"title\": \"S\", \"GlossList\": {\"GlossEntry\": {\"ID\": \"SGML\",	\"SortAs\": \"SGML\", \"GlossTerm\": \"Standard Generalized Markup Language\", \"Acronym\": \"SGML\", \"Abbrev\": \"ISO 8879:1986\", \"GlossDef\": {\"para\": \"A meta-markup language, used to create markup languages such as DocBook.\",\"GlossSeeAlso\": [\"GML\", \"XML\"]}, \"GlossSee\": \"markup\"}}}}}";

مـتم.طـرفية.اطبع(
    كائن_جيسون
        .هات_كائن("glossary")
        .هات_كائن("GlossDiv")
        .هات_كائن("GlossList")
        .هات_كائن("GlossEntry")
        .هات_كائن("GlossDef")
        .هات_كائن("GlossSeeAlso")
        .هات_نص(1)
); // نتيجة: XML

