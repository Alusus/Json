اشمل "مـتم/طـرفية.أسس"؛
اشمل "مـتم/نـص.أسس"؛
اشمل "مـحا"؛
مـحا.اشمل_ملف("Alusus/Json"، "جـيسون.أسس")؛

استخدم مـتم؛

دالة ابدا {
    عرف كائن: جـيسون = "{\n\t\"success\": true,\n\t\"time\": \"2020-01-14T17ZT\",\n\t\"hostname\": \"localhost\"\n}"

    طـرفية.اطبع("key: %s \t\t\t value: %s\n", كائن.مفاتيح(0).صوان, كائن.قيم(0).صوان)
    طـرفية.اطبع("key: %s \t\t\t value: %s\n", كائن.مفاتيح(1).صوان, كائن.قيم(1).صوان)
    طـرفية.اطبع("key: %s \t\t\t value: %s\n", كائن.مفاتيح(2).صوان, كائن.قيم(2).صوان)
}
ابدا()؛

