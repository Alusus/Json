اشمل "مـتم/طـرفية.أسس"؛
اشمل "مـتم/نـص.أسس"؛
اشمل "مـحا"؛
مـحا.اشمل_ملف("Alusus/Json"، "جـيسون.أسس")؛

استخدم مـتم؛
دالة ابدأ {
    عرف جيسون: جـيسون = "{ \"ا\": { \"ب\": \"ا\\\"ب،ت,ث[]\" } }"؛
    طـرفية.اطبع("%s\ج"، جيسون("ا")("ب")~مثل[نـص].صوان)؛
    جيسون = "{ \"مفتاح{مع\\\"\": \"ابت\" }"؛
    طـرفية.اطبع("%s\ج"، جيسون("مفتاح{مع\"")~مثل[نـص].صوان)؛
}
ابدأ();
